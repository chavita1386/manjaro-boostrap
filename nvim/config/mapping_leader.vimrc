" =========================================
" 			 mapping leader
" =========================================

nmap <Leader>w :w<CR> " save file
nmap <Leader>q :q<CR> " close
nmap <Leader>x :bd<CR> " close buffer

" easymotion
nmap <Leader>s <Plug>(easymotion-s2)

" prettier
nmap <Leader>l :Prettier<CR>

" Select all
nmap <C-a> ggVG
